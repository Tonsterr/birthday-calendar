{
  "homepage": "https://tonsterr.github.io/birthday-calendar",
  "name": "birthday-calendar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
